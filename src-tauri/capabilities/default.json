{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "DoctorFill default capabilities",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "shell:allow-open",
    "shell:allow-spawn",
    "shell:allow-execute",
    {
      "identifier": "shell:allow-spawn",
      "allow": [
        {
          "name": "doctorfill-server",
          "cmd": "doctorfill-server",
          "args": true,
          "sidecar": true
        }
      ]
    }
  ]
}
